<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <element>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:reint_downloadmanager/Resources/Private/Language/locallang.xlf:chead1
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <settings.topdnum>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>LLL:EXT:reint_downloadmanager/Resources/Private/Language/locallang.xlf:topdnum
                            </label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <max>100</max>
                                <eval>int</eval>
                                <default>10</default>
                            </config>
                        </TCEforms>
                    </settings.topdnum>

                    <settings.topdtitle>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>LLL:EXT:reint_downloadmanager/Resources/Private/Language/locallang.xlf:topdtitle
                            </label>
                            <config>
                                <type>input</type>
                                <size>60</size>
                                <max>255</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.topdtitle>

                    <settings.lbpid>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>LLL:EXT:reint_downloadmanager/Resources/Private/Language/locallang.xlf:lbrpid</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>sys_file_collection</allowed>
                                <size>10</size>
                                <maxitems>999</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>0</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.lbpid>

                    <settings.dfolder>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>LLL:EXT:reint_downloadmanager/Resources/Private/Language/locallang.xlf:dfolder
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>5</size>
                                <maxitems>999</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>0</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.dfolder>

                </el>
            </ROOT>
        </element>
    </sheets>
</T3DataStructure>
